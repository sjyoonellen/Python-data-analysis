#수기로 2차 Others키워드 발라내서 코드에 넣기


#코드

import pandas as pd
import numpy as np

data=pd.read_csv('C:\\Users\\TZ422RR\\OneDrive - EY\\Desktop\\third.csv', encoding="cp949")

others_keyword = ['usb cable',
 'lightning cable',
 'type-c cable',
 'micro usb cable', 'type c to type c cable', 'USB Adapter', 'type-c to lightning cable', 'hdmi cable', 'vga cable', 
 'usb extension cable', 'multi charging cable', 'data sync cable', 'usb wifi adapter', 'wireless network adapter',
 'wi-fi dangle', 'tv remote', 'universal remote', 'Charging Adapter ','firestick remote', 'swivel tilt wall mount', 
 'full motion cantilever mount', 'hdmi to vga adapter', 'USB 3.0 A to Micro B', 'Data Cable','braided', 'nylon braided', 'tangle-free', 
 'fast charging', 'quick charge', 'pd technology', 'unbreakable', 'l shaped', 'flat cable', 'stress resistant',
 'usb a-male to a-female extension cable','USB Type-C to USB Type-C 2.0 Cable', 'USB Mini Adapter', 'USB A Male Cable', 'USB 2.0 - A-Male to A-Female Extension Cable', 'Type C to C Cable - 3.28 Feet', 'usb a to usb a cable', 'usb 2.0 cable', 'usb 3.0 cable','remote cover',
 'ethernet', 'soft ap mode', 'Cable', 'Remote', 'Adapter', 'Holder', 'Projector', 'Stand', 'Mount', 'Bracket', 'Converter', 'Case', 'Cover', 'Coupler', 'Extension', 'Dongle', 'Receiver', 'Splitter', 'Transmitter', 'Connector', 'Skin'
]

data['Others'] = data.apply(
    lambda row: 'Others' if (
        any(keyword.lower() in row['product_name'].lower() for keyword in others_keyword)) else '-',
        axis=1
)


res2_TV = ["TV"]
res2_Smartphone = ["Smartphone", "iPhone"]
res2_Speaker = ["Speaker"]
res2_Watch=["Watch"]



data['keyword'] = data.apply(
    lambda row: row['keyword'] if row['Others'] == 'Others' else
    ('TV' if any(keyword.lower() in row['product_name'].lower() for keyword in res2_TV) else
    'Smartphone' if any(keyword.lower() in row['product_name'].lower() for keyword in res2_Smartphone) else
    'Speaker' if any(keyword.lower() in row['product_name'].lower() for keyword in res2_Speaker) else
    'Watch' if any(keyword.lower() in row['product_name'].lower() for keyword in res2_Watch) else row['keyword']),
    axis=1
)

data.head()

data['keyword'].unique()

